/* pages/files/files.wxss */
.navigatorBar{
  height: 150rpx; 
  background-color: #87E8DE;
}

.searchBox{
  height: 30rpx;
  margin: 0 30rpx 20rpx 30rpx;
  padding-left: 70rpx;
  padding-right: 40rpx;
  border-radius: 30rpx;
  font-size: 23rpx;
  text-align: left;
  color: #999999;
  background-color: rgba(255, 255, 255, 0.603);
  background-image: url('data:img/jpg;base64,');
  background-repeat: no-repeat;
  background-size: 27rpx;
  background-position-x: 30rpx;
  background-position-y: 13rpx;
}

