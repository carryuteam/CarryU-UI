button.searchButton::after{
border: none;
}

.filter{
  display: flex;
  height: 70rpx;
  margin-left: 30rpx;
}

button.rankerItem{
  height: 50rpx;
  font-size: 27rpx;
  line-height: 50rpx;
  text-align: center;
  background: none;
  color: #333333;
  margin: 6rpx 3rpx 0rpx 5rpx;
}

button.rankerItem::after{
border: none;
}

button.rankerItemSelected{
  height: 50rpx;
  font-size: 27rpx;
  line-height: 50rpx;
  text-align: center;
  background-color: #006D75;
  color: white;
  border-radius: 30rpx;
  margin: 6rpx 5rpx 0rpx 5rpx;
}

button.rankerItemSelected::after{
border: none;
}

.icon-img{
  height: 38rpx;
  width: 38rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
}

.backgroundImage{
  width: 100%;
  height: 100%;
  position: fixed;
  background-size:100% 100%;
  z-index: -1;
}

scroll-view.scrollExplore{
  display: flex;
  flex-direction: column;
} 

.fileView{
  width: 80%;
  height: 170rpx;
  display: flex; 
  flex-direction: column;
  margin: 20rpx 30rpx 20rpx 30rpx;
  padding: 40rpx 50rpx 30rpx 50rpx;
  border-radius: 20rpx;
  background-color: #FFFFFF;
}

.fileTitle{
  height: 40rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  line-height: 30rpx;
  margin-bottom: 15rpx; 
}

.fileTitleText{
  line-height: 30rpx;
  width: 300rpx;
  font-size: 25rpx;
  color: #333333;
}

.fileTitleDetail{
  line-height: 40rpx;
  height: 34rpx;
  width: 20rpx;
  margin-left: 280rpx;
}

.fileLable{
  width: 300rpx;
  font-size: 20rpx;
  color: #999999;
}

.divLine{
 background: #E0E3DA;
 width: 100%;
 height: 5rpx;
 margin-top: 20rpx; 
 margin-bottom: 10rpx;
}

.fileShareTime{
  line-height: 30rpx;
  width: 300rpx;
  font-size: 20rpx;
  color: #999999;
  margin-right: 140rpx;
}

.fileActionImage{
  width: 40rpx; 
  height: 40rpx;
  margin-left: 40rpx;
  align-content: center;
}